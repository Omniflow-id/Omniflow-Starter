{
  "name": "omniflow-starter",
  "version": "1.0.0",
  "description": "The Omniflow ERP module's starter pack",
  "main": "server.js",
  "scripts": {
    "start": "node --no-deprecation server.js",
    "dev": "nodemon --exec \"node --no-deprecation\" server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "biome check --write .",
    "lint": "biome lint ."
  },
  "keywords": [
    "HRIS"
  ],
  "author": "Eric Julianto",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/exporter-prometheus": "^0.202.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/sdk-node": "^0.202.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "chokidar": "^3.6.0",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "knex": "^3.1.0",
    "marked": "^12.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "nunjucks": "^3.2.4",
    "nunjucks-date-filter": "^0.1.1",
    "ua-parser-js": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2"
  },
  "_moduleAliases": {
    "@": ".",
    "@config": "./config",
    "@db": "./db",
    "@helpers": "./helpers",
    "@middlewares": "./middlewares",
    "@routes": "./routes",
    "@views": "./views",
    "@public": "./public"
